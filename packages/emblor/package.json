{
  "name": "emblor",
  "version": "1.4.8",
  "description": "A full featured, unstyled tag input component for React",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "turbo build --filter emblor",
    "typecheck": "tsc --noEmit",
    "build": "run-p build:*",
    "build:tsup": "tsup --dts --minify",
    "clean": "rimraf dist",
    "dev": "tsup --watch --dts",
    "format": "prettier --write .",
    "prettier-check": "prettier --check \"**/*.{ts,tsx,md}\"",
    "release": "npm run build && changeset publish"
  },
  "keywords": [
    "shadcn",
    "react",
    "tag input",
    "shadcn/ui",
    "tag input component",
    "shadcn tag input component",
    "input",
    "radix ui",
    "react tag input",
    "tag input component",
    "emblor",
    "emblor tag input component"
  ],
  "author": "Jaleel Bennett <jaleelcodes@gmail.com>",
  "license": "MIT",
  "homepage": "https://emblor.jaleelbennett.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/JaleelB/emblor",
    "directory": "packages/emblor"
  },
  "bugs": {
    "url": "https://github.com/JaleelB/emblor/issues"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "tsup": "^8.0.0",
    "typescript": "^5.6.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "packageManager": "pnpm@9.0.2",
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.4",
    "class-variance-authority": "latest",
    "clsx": "latest",
    "cmdk": "^1.1.1",
    "react-easy-sort": "^1.6.0",
    "tailwind-merge": "latest"
  }
}
